@model IEnumerable<Carpool.Models.Lift>
<body>
    <br />
    <h4>Past Lifts</h4>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StartPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndPoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LiftDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OutboundStartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OutboundEndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InboundStartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InboundEndTime)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StartPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EndPoint)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LiftDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OutboundStartTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OutboundEndTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InboundStartTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InboundEndTime)
                </td>
                <td>
                    @Html.ActionLink("View Requests", "ViewRequests", "Home", new { id = item.LiftID }, null) |
                    @Html.ActionLink("Accepted Requests", "AcceptedRequests", "Home", new { id = item.LiftID }, null) |
                    @Html.ActionLink("Rejected Requests", "RejectedRequests", "Home", new { id = item.LiftID },null ) |
                    @Html.ActionLink("View Lift", "ViewLift", "Home", new { id = item.LiftID }, null) |
                    @Html.ActionLink("Delete", "Delete", "Home", new { id = item.LiftID, @class = "btn-danger btn-xs" }, null)
                </td>
            </tr>
        }

    </table>

</body>
